Question: 1
function reverseArray(arr) 
{
    let reverseArray = [];
    for (let i = arr.length - 1; i >= 0; i--) 
    {
        reverseArray.push(arr[i]);
    }
    return reverseArray;
}

const inputArray = [1, 2, 3];
console.log('Input:', inputArray);
console.log('Reversed:', reverseArray(inputArray));
